<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitplay_app"
    android:versionCode="1"
    android:versionName="1.0">

    <uses-sdk
        android:minSdkVersion="13"
        android:targetSdkVersion="19" />
	<uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="run_fitplay_app.IntroScreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        
        <activity
            android:name="run_fitplay_app.MainActivity"
            android:label="@string/app_name" >
        </activity>
        
        <activity
            android:name="run_fitplay_app.SecondScreen"
            android:label="@string/title_activity_second_screen">
        </activity>
        
        <activity
            android:name="application.presentation.group.ListGroups"
            android:label="@string/title_activity_list_groups" >
        </activity>
        
        <activity
            android:name="application.presentation.group.NewGroup"
            android:label="@string/title_activity_new_group">
        </activity>
        
        <activity
            android:name="application.presentation.users.NewMembers"
            android:label="@string/title_activity_new_members">
        </activity>
        
        <activity
            android:name="application.presentation.challenge.CreateChallege"
            android:label="@string/title_activity_create_challege" >
        </activity>
        
        <activity
            android:name="application.presentation.challenge.DescribeChallenge"
            android:label="@string/title_activity_describe_challenge" >
        </activity>
        
        <activity
            android:name="application.presentation.challenge.FinishChallenge"
            android:label="@string/title_activity_finish_challenge" >
        </activity>
        
        <activity
            android:name="application.presentation.group.DescribeGroup"
            android:label="@string/title_activity_describe_group"
            android:parentActivityName=".ListGroups" >
        </activity> 
          
        <activity
            android:name="application.presentation.log.FitnessTracking"
            android:label="@string/title_activity_fitness_tracking" >
        </activity>
             
        <activity
            android:name="application.presentation.activites.ChooseActivityType"
            android:label="@string/title_activity_choose_activity_type" >
        </activity>
        
        <activity
            android:name="application.presentation.activites.NewCardioActivity"
            android:label="@string/title_activity_new_cardio" >
        </activity>
        
        <activity
            android:name="application.presentation.activites.NewStrengthActivity"
            android:label="@string/title_activity_new_strength" >
        </activity>
        
        <activity
            android:name="application.presentation.log.LogCardio"
            android:label="@string/title_activity_log_cardio" >
        </activity>
        
        <activity
            android:name="application.presentation.log.LogStrength"
            android:label="@string/title_activity_log_strength" >
        </activity>
        
        <activity 
            android:name="application.presentation.activites.CardioActivities"
            android:label="@string/title_activity_cardio_activities" >
        </activity>
        
        <activity
            android:name="application.presentation.activites.StrengthActivities"
            android:label="@string/title_activity_strength_activities" >
        </activity>

        <activity
            android:name="application.presentation.group.GroupData"
            android:label="@string/title_activity_group_data" >
        </activity>
    </application>

</manifest>
